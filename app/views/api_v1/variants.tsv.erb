<%= %w[hgvs chromosome start stop read variant reference_version gene mutation_type amino_acid diseases pubmed_sources].join("\t") %>
<% @variants.each do |variant| %>
<%=
  [
    variant.hgvs,
    variant.location.chromosome,
    variant.location.start,
    variant.location.stop,
    variant.location.reference_read,
    variant.variant,
    variant.location.reference_sequence_version,
    variant.gene.name,
    variant.mutation_type.name,
    variant.amino_acid.name,
    variant.diseases.map(&:name),
    variant.disease_sources.map(&:pubmed_id)
  ].join("\t")
%>
<% end %>
